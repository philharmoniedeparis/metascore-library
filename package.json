{
  "name": "metascore",
  "version": "1.2.9",
  "description": "metaScore",
  "homepage": "http://metascore.philharmoniedeparis.fr",
  "author": "Oussama Mubarak",
  "license": "CECILL-2.1",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "pack": "webpack -p",
    "dev": "webpack -d -w",
    "lint": "eslint -c .eslintrc.json src/js",
    "doc": "esdoc",
    "drupal": "git-branch-is -q drupal-8 && npm run drupal-8 || npm run drupal-7",
    "drupal-7": "copyfiles -u 1 dist/**/* ../Drupal.git/sites/default/libraries/metaScore",
    "drupal-8": "copyfiles -u 1 dist/**/* ../Drupal-8.git/web/modules/custom/metascore_library/vendor/metaScore"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:philharmoniedeparis/metascore-library.git"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "beepbeep": "^1.3.0",
    "copyfiles": "^2.1.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^3.0.1",
    "filenamify": "^4.1.0",
    "git-branch-is": "^2.1.0",
    "git-rev-sync": "^1.12.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "string-replace-loader": "^2.2.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo-loader": "^2.2.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "fuse.js": "^3.4.5",
    "lottie-web": "^5.5.9",
    "resize-observer-polyfill": "^1.5.1",
    "waveform-data": "^2.1.2"
  }
}
