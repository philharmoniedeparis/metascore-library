{
  "name": "metaScore",
  "version": "1.2.9",
  "description": "metaScore",
  "homepage": "http://metascore.philharmoniedeparis.fr",
  "author": "Oussama Mubarak",
  "license": "CECILL-2.1",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "pack": "webpack -p",
    "dev": "webpack -d -w",
    "lint": "eslint -c .eslintrc.json src/js",
    "doc": "esdoc",
    "i18n": "node ./bin/i18n-extract.js",
    "drupal": "git-branch-is -q drupal-8 && npm run drupal-8 || npm run drupal-7",
    "drupal-7": "copyfiles -u 1 dist/**/* ../Drupal.git/sites/default/libraries/metaScore",
    "drupal-8": "copyfiles -u 1 dist/**/* ../Drupal-8.git/web/sites/all/assets/vendor/metaScore"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:philharmoniedeparis/metascore-library.git"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "beepbeep": "^1.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "git-branch-is": "^2.1.0",
    "git-rev-sync": "^1.12.0",
    "image-webpack-loader": "^4.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "waveform-data": "^2.1.2"
  }
}
