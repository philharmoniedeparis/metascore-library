    "cross-env": "^7.0.3",
    "cypress": "^14.2.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.81.0",
    "start-server-and-test": "^2.0.9",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-vue-devtools": "^7.6.8",
