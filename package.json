    "ajv-formats": "^3.0.1",
    "ajv-i18n": "^4.2.0",
    "blockly": "^11.1.1",
    "chroma-js": "^3.1.2",
    "ckeditor5": "^44.0.0",
    "core-js": "^3.25.1",
    "dashjs": "^5.0.0",
    "fuse.js": "^7.0.0",
    "geometry-polyfill": "^1.0.7",
    "hammerjs": "^2.0.8",
    "hls.js": "^1.1.5",
    "lodash": "^4.17.21",
    "lottie-web": "^5.8.1",
    "mixwith.ts": "^1.1.3",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.1",
    "sortablejs": "^1.15.2",
    "source-sans": "github:adobe-fonts/source-sans#3.046R",
    "timecode-input": "github:philharmoniedeparis/timecode-input#v2.0.1",
    "tiny-emitter": "^2.1.0",
    "uuid": "^11.0.3",
    "v-hotkey": "https://github.com/philharmoniedeparis/v-hotkey/tarball/2587857",
    "validate-color": "2.2.1",
    "vue": "^3.5.12",
    "vue-dompurify-html": "^5.0.0",
    "vue-i18n": "^11.1.2",
    "waveform-data": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@interactjs/dev-tools": "^1.10.27",
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@pinia/testing": "^1.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.13",
    "@types/metascore-module-manager": "file:src/core/services/module-manager/ts-plugin",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "^1.1.16",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
