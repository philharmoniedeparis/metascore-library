    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:compat/recommended"
  ],
  "globals": {
  },
  "rules": {
  },
  "settings": {
    "polyfills": [
      "es:all",
      "DOMMatrix",
      "DOMPoint"
    ]
  }
}
